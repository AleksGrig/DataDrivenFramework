<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Learning">

	<groups>
		<run>
			<include name="functional"></include>
		</run>
	</groups>

	<test thread-count="5" name="Test">
		<parameter name="browser" value="firefox"></parameter>

		<classes>
			<class name="com.automation.rough.TestMethodDependencies" />
			<class name="com.automation.rough.TestSoftAssert" />
			<class name="com.automation.rough.TestSkip" />
			<class name="com.automation.rough.TestParametersFromXML" />
		</classes>
	</test> <!-- Test -->
	
</suite> <!-- Learning -->
